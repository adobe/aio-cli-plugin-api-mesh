		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.21.2",
		"eslint-plugin-jest": "^23.13.2",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-no-loops": "^0.3.0",
		"eslint-plugin-node": "^11.1.0",
